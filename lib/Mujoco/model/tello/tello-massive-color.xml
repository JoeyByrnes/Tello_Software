        <geom contype='2' conaffinity='4' solimp="1 1 0.005"/>
      </default>
      <default class='collision-right'>
        <geom contype='4' conaffinity='2' solimp="1 1 0.005"/>
      </default>
    </default>
    <default class='obstacle'>
      <geom material='obstacle' condim='3' contype='8' conaffinity='15' user='1' solimp="1 1 0.005"/>
    </default>
  </default>
  
